<% @recommended_spotify_users.each do |user| %>
  <% if user.images.first %>
    <%= image_tag(user.images.first['url']) %>
  <% end %>
  <%= user.id %><br/><br/>
  <% @recommended_spotify_artists[user.id].each do |artist| %>
    <% if artist %>
      <table class="table table-hover">
        <tr class="tablerow">
           <td><iframe src="https://embed.spotify.com/?uri=<%= artist.albums.first.tracks.first.uri%>" width="100%" height="80"  frameborder="0" allowtransparency="true"></iframe></td>
        </tr>
    <% end %><br/><br/>
  <% end %><br/><br/>
<% end %>
